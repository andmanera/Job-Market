\begin_layout Proof
\begin_inset Formula 
\begin{align}
\rho v(1)-\pi-\frac{\gamma-1}{\gamma}\left(\frac{v(\omega)-v(1)}{\alpha_{I}w^{RD}}\right)^{\frac{\gamma}{\gamma-1}}+\frac{v(1)^{2}}{\zeta w^{RD}} & =0\label{eq: sys1-3}\\
\rho v(\omega)-\pi-\delta\left(v(1)-v(\omega)\right)+\frac{v(1)}{\zeta w^{RD}\omega}v(\omega) & =0.\label{eq: sys2-3}\\
\rho\left(\frac{v(1)}{w^{RD}}+\mu_{\omega}\frac{v(\omega)-v(1)}{w^{RD}}\right)-\frac{\pi}{w^{RD}}+L^{RD}(w^{RD}) & =0.\label{eq:sys3-3}
\end{align}

\end_inset

Now the IFT accounting for the wage changes is:
\begin_inset Foot
status collapsed

\begin_layout Plain Layout
Note:
\begin_inset Formula 
\begin{align*}
\frac{1}{w^{RD}}\frac{v(1)v(\omega)}{\zeta w^{RD}\omega} & =x_{e,\omega}\left(\frac{v(\omega)}{w^{RD}}\right)=x_{e,\omega}\left(\frac{v(\omega)-v(1)}{w^{RD}}\right)+\zeta\omega x_{e,\omega}^{2}\\
 & =\alpha_{I}x_{e,\omega}x_{I}^{\gamma-1}+\zeta\omega x_{e,\omega}^{2}
\end{align*}

\end_inset


\end_layout

\end_inset


\begin_inset Formula 
\begin{align*}
\left[\begin{matrix}\rho+\left(\frac{v(\omega)-v(1)}{\alpha_{I}w^{RD}}\right)^{\frac{1}{\gamma-1}}+2\frac{v(1)}{\zeta w^{RD}} & -\left(\frac{v(\omega)-v(1)}{\alpha_{I}w^{RD}}\right)^{\frac{1}{\gamma-1}} & \frac{1}{w^{RD}}\left(\frac{v(\omega)-v(1)}{\alpha_{I}w^{RD}}\right)^{\frac{\gamma}{\gamma-1}}-\frac{1}{w^{RD}}\frac{v(1)^{2}}{\zeta w^{RD}}\\
-\delta+\frac{v(\omega)}{\zeta w^{RD}\omega} & \rho+\delta+\frac{v(1)}{\zeta w^{RD}\omega} & \frac{1}{w^{RD}}\frac{v(1)v(\omega)}{\zeta w^{RD}\omega}\\
\rho\frac{\mu_{1}}{w^{RD}}+\rho\frac{\partial\mu_{\omega}}{\partial v(1)}\frac{v(\omega)-v(1)}{w^{RD}} & \rho\frac{\mu_{\omega}}{w^{RD}}+\rho\frac{\partial\mu_{\omega}}{\partial v(\omega)}\frac{v(\omega)-v(1)}{w^{RD}} & \frac{1}{w^{RD}}\frac{\pi}{w^{RD}}-\frac{\partial L^{RD}}{\partial w^{RD}}
\end{matrix}\right]\left[\begin{matrix}\mathrm{d}v(1)\\
\mathrm{d}v(\omega)\\
\mathrm{d}w^{RD}
\end{matrix}\right]-\left[\begin{matrix}1\\
1\\
\frac{1}{w^{RD}}
\end{matrix}\right]\mathrm{d}\pi & =0\\
\left[\begin{matrix}\rho+x_{I}+2\omega x_{e,\omega} & -x_{I} & \frac{1}{w^{RD}}x_{I}^{\gamma}-\zeta\omega^{2}x_{e,\omega}^{2}\\
-\delta+x_{e,\omega} & \rho+\delta+x_{e,\omega} & \alpha_{I}x_{e,\omega}x_{I}^{\gamma-1}+\zeta\omega x_{e,\omega}^{2}\\
\rho\frac{\mu_{1}}{w^{RD}}+\rho\frac{\partial\mu_{\omega}}{\partial v(1)}\alpha_{I}x_{I}^{\gamma-1} & \rho\frac{\mu_{\omega}}{w^{RD}}+\rho\frac{\partial\mu_{\omega}}{\partial v(\omega)}\alpha_{I}x_{I}^{\gamma-1} & \frac{1}{w^{RD}}\frac{\pi}{w^{RD}}+\frac{\partial L^{RD}}{\partial w^{RD}}
\end{matrix}\right]\left[\begin{matrix}\mathrm{d}v(1)\\
\mathrm{d}v(\omega)\\
\mathrm{d}w^{RD}
\end{matrix}\right]-\left[\begin{matrix}1\\
1\\
\frac{1}{w^{RD}}
\end{matrix}\right]\mathrm{d}\pi & =0
\end{align*}

\end_inset

